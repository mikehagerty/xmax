<?xml version="1.0" ?> 
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 0.5" 
"http://jpf.sourceforge.net/plugin_0_5.dtd"> 
<plugin id="com.isti.xmax.filterHP" version="1.0"> 
	<requires> 
		<import plugin-id="com.isti.xmax.core"/> 
	</requires> 
	<runtime> 
		<library id="filterHP" path="classes/" type="code"/> 
	</runtime> 
	<extension plugin-id="com.isti.xmax.core" point-id="Filter" id="HP"> 
		<parameter id="class" value="FilterHP"/>
		<parameter id="name" value="HP filter"/> 
		<parameter id="description" value="Apply High Pass filter for selected channels"/>  
	</extension> 
</plugin> 